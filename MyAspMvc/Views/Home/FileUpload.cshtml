@{
    ViewData["Title"] = "FileUpload";
}

<h1>FileUpload</h1>

<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="FileUpload">
    <div class="form-group">
        <input type="file" name="file">
    </div>
    <div class="form-group">
        <input type="submit" value="Upload" />
    </div>
    <div class="form-group">
        <p>@TempData["msg"]</p>
    </div>
</form>

